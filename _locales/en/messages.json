	"sql_tbl_todo": {
		"message": "CREATE TABLE IF NOT EXISTS todo(id INTEGER NOT NULL PRIMARY KEY, uid TEXT, status INTEGER NOT NULL, done INTEGER NOT NULL, url TEXT, created INTEGER NOT NULL, dtstart INTEGER NOT NULL, dtend INTEGER, location TEXT, lat REAL, lon REAL, title TEXT, summary TEXT, description TEXT, userid TEXT, orgcn TEXT, orgemail TEXT, orgphone TEXT, rem TEXT, ics TEXT);"
	},
	"sql_ins_todo": {
		"message": "INSERT INTO todo(uid, status, done, url, created, dtstart, dtend, location, lat, lon, title, summary, description, userid, orgcn, orgemail, orgphone, rem, ics) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?);"
	},
	"sql_del_done": {
		"message": "DELETE FROM todo WHERE done = ?;"
	},
	"sql_upd_done": {
		"message": "UPDATE todo SET done = 1 WHERE id = ?;"
	},
	"sql_sel_todo": {
		"message": "SELECT id, uid, status, done, url, dtstart, dtend, location, lat, lon, title, summary, description, userid, orgcn, orgemail, orgphone, rem, ics FROM todo WHERE done = ? ORDER BY dtstart;"
	},
